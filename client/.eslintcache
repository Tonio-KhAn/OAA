icated) {\r\n    return <Redirect push to=\"/profile\" />;\r\n    }\r\n  return (\r\n    <>\r\n      <div className='form-container'>\r\n        <span className='close-btn'><Link to='/'>x</Link></span>\r\n        <div className='form-content-left'>\r\n          <img className='form-img' alt='DCIT Picture' />\r\n        </div>\r\n          <FormLogin submitForm={submitForm}/>\r\n        </div>\r\n    </>\r\n  );\r\n}\r\n\r\nconst mapStateToProps = state =>({\r\n  isAuthenicated: state.auth.isAuthenticated,\r\n  error:state.error\r\n}); \r\n\r\nconst mapDispatchToProps = dispatch => {\r\n  return{\r\n    login: (data) => dispatch(login(data))  }\r\n}\r\n\r\nexport default connect(\r\n  mapStateToProps,\r\n  mapDispatchToProps)\r\n  (Login)\r\n","C:\\Users\\Tonio\\Desktop\\UWI\\final year\\oAA\\OAA\\client\\src\\components\\Cards.js",[],"C:\\Users\\Tonio\\Desktop\\UWI\\final year\\oAA\\OAA\\client\\src\\components\\HeroSection.js",[],"C:\\Users\\Tonio\\Desktop\\UWI\\final year\\oAA\\OAA\\client\\src\\components\\Button.js",[],"C:\\Users\\Tonio\\Desktop\\UWI\\final year\\oAA\\OAA\\client\\src\\components\\forms\\FormSignUpEmail.js",[],"C:\\Users\\Tonio\\Desktop\\UWI\\final year\\oAA\\OAA\\client\\src\\components\\forms\\FormSuccess.js",[],"C:\\Users\\Tonio\\Desktop\\UWI\\final year\\oAA\\OAA\\client\\src\\components\\forms\\FormSignup.js",[],"C:\\Users\\Tonio\\Desktop\\UWI\\final year\\oAA\\OAA\\client\\src\\components\\forms\\FormLogin.js",[],"C:\\Users\\Tonio\\Desktop\\UWI\\final year\\oAA\\OAA\\client\\src\\components\\CardItem.js",[],"C:\\Users\\Tonio\\Desktop\\UWI\\final year\\oAA\\OAA\\client\\src\\components\\validate\\validateInfoSignUp.js",[],"C:\\Users\\Tonio\\Desktop\\UWI\\final year\\oAA\\OAA\\client\\src\\components\\validate\\validateEmail.js",[],"C:\\Users\\Tonio\\Desktop\\UWI\\final year\\oAA\\OAA\\client\\src\\components\\validate\\validateLogin.js",[],"C:\\Users\\Tonio\\Desktop\\UWI\\final year\\oAA\\OAA\\client\\src\\components\\pages\\createPost.js",